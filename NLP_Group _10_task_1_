n","    print(\"✅ تم تحميل نموذج BERT بنجاح!\")\n","except Exception as e:\n","    print(f\"❌ خطأ في تحميل النموذج: {e}\")\n","\n","# ✅ 3. دالة التلخيص باستخدام BERT\n","def summarize_text_with_pipeline(text):\n","    try:\n","        summary = summarizer(\n","            text,\n","            max_length=50,  # الحد الأقصى لطول التلخيص\n","            min_length=10,  # الحد الأدنى لطول التلخيص\n","            do_sample=False,\n","            truncation=True  # اقتطاع النص الطويل\n","        )\n","        return summary[0]['summary_text']\n","    except Exception as e:\n","        print(f\"❌ خطأ في التلخيص: {e}\")\n","        return text  # إذا حدث خطأ، يتم إرجاع النص الأصلي\n","\n","# ✅ 4. تطبيق التلخيص على النصوص الأصلية\n","df['summary_generated'] = df['Text'].apply(summarize_text_with_pipeline)\n","\n","# ✅ 5. عرض النتائج\n","for index, row in df.iterrows():\n","    print(f\"📄 النص الأصلي:\\n{row['Text']}\\n\")\n","    print(f\"✍️ التلخيص (باستخدام BERT):\\n{row['summary_generated']}\\n\")\n","    print(\"-\" * 80)\n","\n","# ✅ 6. حفظ النتائج في ملف جديد\n","df.to_csv('/content/bert_raw_text_summarized.csv', index=False)\n","print(\"✅ تم حفظ النتائج في ملف 'bert_raw_text_summarized.csv'\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":960,"referenced_widgets":["2b8176d3220245b8a1526ae829a02916","82a0601502004b679225c4128b7e323c","80beee57f9874fdba5fb52fe54e01416","1e931728f5844812b06b91de54beb2c9","7a895d7c31834f2dac12207e00604290","976ed3906abd4945bd31a18c5504d238","a499f5f0112d4b67867270d060c11c93","f1d9e08dcb2b4a7bad7144d1016f27fb","6ff183f3861c43e096ef0466b6641647","025c29dd05b54de0acc7412de1ba1ae3","d5423676bf424121ae6a8e79b7226286","7fd51015448e41d2b29d9e29d6774f44","71faf3d9df4746b9981d5b0588415259","6966d4c32af04167bd3e9bda4cc5eeb5","a2c71aacec5545a1bf641abf9237237c","099e501969254554bd14f3ce43c482e1","049a259cb6534764b85b971c80b67313","b1985d02a9fa460a8b385b82311f1f94","d30c501bd65c4b7196970efd98953d9c","6b34006cfd7a4df08459fd884